[
    {
      "name": "orders-api",
      "image": "148761658682.dkr.ecr.us-east-1.amazonaws.com/orders-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3002,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "PORT", "value": "3002" },
        { "name": "SERVICE_TOKEN", "value": "internal-service-token" },
        { "name": "CUSTOMERS_API_BASE", "value": "http://customers-api:3001" }
      ]
    }
  ]